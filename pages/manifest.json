{
	"manifest_version": 2,
	"name": "Stinger Launcher",
	"description": "Oh shit! I'm sorry",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"offline_enabled" : true,
  "options_page": "options/options.html",  
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions" : [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "storage",
    "tabs",
    "*://*/*"
  ]
}